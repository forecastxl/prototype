{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": { "browsers": [ "last 2 versions", "> 5%" ] },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "presets": [
        "env"
      ]
    }
  }
}
