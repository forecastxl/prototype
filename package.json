{
  "author": "forecastxl",
  "name": "prototype",
  "version": "1.0.0",
  "scripts": {
    "start": "DEV_BASE=http://localhost DEV_PORT=8080 API=https://developapi.forecastxl.com/api/v2 webpack-dev-server --content-base dist/ --inline --hot",
    "build:master": "NODE_ENV=production API=https://api.forecastxl.com/api/v2 webpack -p --bail",
    "build:staging": "NODE_ENV=production API=https://stagingapi.forecastxl.com/api/v2 webpack -p --bail",
    "build:develop": "NODE_ENV=production API=https://developapi.forecastxl.com/api/v2 webpack -p --bail",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint:js": "eslint 'client/**/*.{js,jsx}'",
    "lint:css": "stylelint 'client/**/*.{js,jsx}'",
    "lint:whitespace": "eclint check client/** server/*.js",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:forecastxl/prototype.git"
  },
  "jest": {
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}",
      "!client/index.jsx",
      "!client/{styles,rootReducer,rootSaga,store}.js",
      "!client/{components,data,services}/*/index.js"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "eslint-plugin-import",
      "eslint-plugin-react",
      "eslint-plugin-jsx-a11y"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.16.0",
    "clean-webpack-plugin": "^0.1.14",
    "coveralls": "^2.11.15",
    "css-loader": "^0.28.0",
    "eclint": "^2.0.1",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.4",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.4.1",
    "style-loader": "^0.17.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-no-unsupported-browser-features": "^0.1.0",
    "stylelint-order": "^0.4.3",
    "stylelint-processor-styled-components": "^0.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "camelcase": "^4.0.0",
    "connected-react-router": "^4.0.0",
    "decamelize": "^1.2.0",
    "isomorphic-fetch": "^2.2.1",
    "map-obj": "^2.0.0",
    "material-ui": "^0.17.1",
    "normalize.css": "^6.0.0",
    "react": "^15.4.1",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.1",
    "react-notification-system": "^0.2.13",
    "react-notification-system-redux": "^1.0.11",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-devtools-extension": "^1.0.0",
    "redux-form": "^6.6.0",
    "redux-form-submit-saga": "^1.1.3",
    "redux-saga": "^0.14.0",
    "styled-components": "^1.2.1"
  }
}
