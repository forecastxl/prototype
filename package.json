    "babel-preset-stage-1": "^6.16.0",
    "clean-webpack-plugin": "^0.1.14",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.0",
    "dotenv": "^2.0.0",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.4",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
